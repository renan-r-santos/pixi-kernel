{
  "argv": [
    "pixi",
    "run",
    "--manifest-path={manifest-path}",
    "R",
    "--slave",
    "-e",
    "'IRkernel::main()'",
    "--args",
    "{connection_file}"
  ],
  "display_name": "Pixi - R",
  "language": "R",
  "env": {
    "R_LIBS": "{manifest-path-parent}/.pixi/envs/default/lib/R/site-library",
    "R_LIBS_SITE": "{manifest-path-parent}/.pixi/envs/default/lib/R/site-library",
    "R_LIBS_USER": "{manifest-path-parent}/.pixi/envs/default/lib/R/site-library"
  },
  "metadata": {
    "pixi-kernel": {
      "package-name": "r-irkernel"
    },
    "kernel_provisioner": {
      "provisioner_name": "pixi-provisioner",
      "config": {}
    }
  }
}
